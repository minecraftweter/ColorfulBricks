{
	"schemaVersion": 1,
	"id": "colorfulbricks",
	"version": "${version}",
	"name": "Colorful Bricks",
	"description": "This mod changes the bricks to be dyable. You can also craft new blocks with the dyed bricks.",
	"authors": [
		"Minecraftweter"
	],
	"contact": {
		"homepage": "https://www.youtube.com/@minecraftbuilds-c6j",
		"sources": "https://github.com/minecraftweter/ColorfulBricks"
	},
	"license": "CC0-1.0",
	"icon": "assets/colorfulbricks/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.minecraftweter.colorfulbricks.ColorfulBricks"
		],
		"fabric-datagen": [
			"net.minecraftweter.colorfulbricks.ColorfulBricksDataGenerator"
		],
		"client": [
			"net.minecraftweter.colorfulbricks.ColorfulBricksClient"
		]
	},
	"mixins": [
		"colorfulbricks.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.18.4",
		"minecraft": "~1.21",
		"java": ">=21",
		"fabric-api": "*"
	}
}